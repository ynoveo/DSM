<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="YNV.DSM.controller.dataset_filelds"
	xmlns:html="http://www.w3.org/1999/xhtml">

			<Page title="{i18n>EditDataSet}">
				<content>
					<Wizard id="CreateProductWizard" complete="wizardCompletedHandler">
						<WizardStep id="DSMdefinition" title="{i18n>EditDataSet}" validated="true">								
							<core:Fragment fragmentName="YNV.DSM.view.DS_M_WIZ.DS_M_Definition" type="XML" />
						</WizardStep>
						<WizardStep id="DSMsourceSystem" title="{i18n>SourceSystem}" validated="true">								
							<core:Fragment fragmentName="YNV.DSM.view.DS_M_WIZ.DS_M_SrcSyst" type="XML" />
						</WizardStep>
						<WizardStep id="DSMdefineTable" title="{i18n>DefineTable}" validated="true">								
							<core:Fragment fragmentName="YNV.DSM.view.DS_M_WIZ.DS_M_Tables" type="XML" />							
						</WizardStep>					
						<WizardStep id="DSMdefineJoin" title="{i18n>DefineJoins}" validated="true">								
							<core:Fragment fragmentName="YNV.DSM.view.DS_M_WIZ.DS_M_Joins" type="XML" />						
						</WizardStep>						
						<WizardStep id="DSMdefineFields" title="{i18n>DefineFields}" validated="true">								
							<core:Fragment fragmentName="YNV.DSM.view.DS_M_WIZ.DS_M_Fields" type="XML" />
							
						</WizardStep>						
					</Wizard>	
				</content>
			</Page>
</mvc:View>